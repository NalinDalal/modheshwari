{
  "name": "modheshwari",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "eslint . --ext .ts,.js,.tsx,.jsx",
    "lint:fix": "eslint . --ext .ts,.js,.tsx,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "docs:gen": "bun run packages/scripts/generate-docs.ts"
  },
  "devDependencies": {
    "@prisma/client": "6.18.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "jsdoc": "^4.0.5",
    "prettier": "^3.6.2",
    "prisma": "6.18.0",
    "ts-morph": "22",
    "turbo": "^2.5.8",
    "typedoc": "0.27",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.2.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@sinclair/typebox": "0.32.10",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.3",
    "elysia": "1.1.3",
    "jsonwebtoken": "^9.0.2"
  }
}
